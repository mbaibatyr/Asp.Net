@helper getList(string[] array)
{
    foreach (string item in array)
    {
        <li>@item</li>
    }
}


@helper Sum(int a, int b)
{
    int z = a + b;
    <h4>@a + @b = @z</h4>
}


@helper getListCar(IEnumerable<MyRazor.Models.Car> array)
{
    foreach (MyRazor.Models.Car item in array)
    {
        <li>@item.BrandModel</li>
    }
}

